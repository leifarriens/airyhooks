{
  "name": "airhooks",
  "version": "0.1.0",
  "description": "CLI to add React hooks to your project",
  "type": "module",
  "bin": {
    "airhooks": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
